{
	"name": "Send to Prowl",
	"version": "1.2",
	"manifest_version": 2,
	"icons": {
		"48": "prowl-cloud-48.png",
		"128": "prowl-cloud-128.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"notifications",
		"https://api.prowlapp.com/"
	],
	"browser_action": {
		"default_icon": "56-cloud.png",
		"default_title": "Send to Prowl"
	},
	"chrome_url_overrides" : {
    	"newtab": "newtab.html"
  	},
  	"web_accessible_resources": [
  		"56-cloud.png"
  	]
}